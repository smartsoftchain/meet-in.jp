{
  "name": "SkyWay画面共有",
  "short_name": "SkyWay画面共有",
  "version": "1.0",
  "manifest_version": 2,
  "description": "SkyWay画面共有",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "desktopCapture",
    "tabs",
    "https://*/*" // in case of inline installation, you have to set your webapp's origin here.
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["https://*/*"], // change origin name as your own.
    "js": ["content.js"],
    "all_frames": true,
    "run_at": "document_end"
  }]
}
