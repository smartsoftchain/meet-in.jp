{
  "name": "Meetin画面共有",
  "short_name": "Meetin画面共有",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Meetin画面共有",
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "desktopCapture",
    "tabs",
    "storage",
    "https://delphinus.sense.co.jp/room/*",
    "https://delphinus2.sense.co.jp/room/*",
    "https://*.meet-in.jp/room/*",
    "https://meet-in.jp/room/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://delphinus.sense.co.jp/*",
        "https://delphinus2.sense.co.jp/*",
        "https://*.meet-in.jp/*",
        "https://meet-in.jp/*"
      ],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
}
